pipeline {
    agent any 
    stages {
        stage('compile and clean') { 
            steps {
                
                sh "mvn clean compile"
            }
        }
        stage('Test') { 
            steps {
                sh "mvn test"
            }
        }
        stage('Deployy') { 
            steps {
                sh "mvn package"
            }
        }
        stage('Archaving') { 
            steps {
               archiveArtifacts "/var/lib/jenkins/workspace/jenkinss-maven/target/my-app-1.0-SNAPSHOT.jar"
            }
        }
    }
}
