pipeline {
    agent any 
    stages {
        stage('compile and clean') { 
            steps {
                
                sh "mvn clean compile"
            }
        }
        stage('Test') { 
            steps {
                sh "mvn test Test"
            }
        }
        stage('Deployy') { 
            steps {
                sh "mvn package -f Deploy"
            }
        }
    }
}
